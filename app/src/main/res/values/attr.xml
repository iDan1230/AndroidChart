<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnnulusView">
        <attr name="radius" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="CustomBar">
        <attr name="progressHeight" format="dimension"/>
        <attr name="cursorWidth" format="dimension"/>
        <attr name="cursorHeight" format="dimension"/>
        <attr name="pcHeight" format="dimension"/>
        <attr name="background_color" format="reference|color"/>
        <attr name="progress_color" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="BrokenLineBar">
        <attr name="coordinateColor" format="reference|color"/>
        <attr name="brokenColor" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="AnnulusChart">

        <attr name="annulus_radius" format="dimension"/>
        <attr name="annulus_width" format="dimension"/>
        <attr name="pointradius" format="dimension"/>
        <attr name="pointtext" format="dimension"/>
        <attr name="annuluspoint" format="dimension"/>

    </declare-styleable>

</resources>